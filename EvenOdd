using System;
using System.Collections.Genric;

void main(String args){

Console.Writeln("Printing first  100 even and odd numbers");

Console.Writeln("Even Numbers");
for(int i=1;i<101;i++){
if(i%2==0){
Console.Writeln(i);
}

Console.Writeln("Odd Numbers");
for(int i=0;i<101;i++){
if(i%2!=0){
Console.Writeln(i);
}
}

}

